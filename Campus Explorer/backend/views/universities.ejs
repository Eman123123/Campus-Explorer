<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>University Data</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      padding: 20px;
    }
    table {
      border-collapse: collapse;
      width: 100%;
    }
    th, td {
      padding: 10px;
      text-align: left;
      border: 1px solid #ccc;
    }
    th {
      background-color: #f4f4f4;
    }
  </style>
</head>
<body>

<h2>University Data</h2>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Admission Date</th>
    </tr>
  </thead>
  <tbody>
    <% universities.forEach(function(u) { %>
      <tr>
        <td><%= u.name || 'N/A' %></td>
        <td><%= u.location || 'N/A' %></td>
        <td><%= u.admissionDate || 'N/A' %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

</body>
</html>
